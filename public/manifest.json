    }
  ],
  "prefer_related_applications": false,
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "file_handlers": [
    {
      "action": "/open-file",
      "accept": {
        "application/pdf": [
          ".pdf"
        ],
        "image/jpeg": [
          ".jpg",
          ".jpeg"
        ],
        "image/png": [
          ".png"
        ]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+urbanauto",
      "url": "/handle-protocol?url=%s"
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "iarc_rating_id": "3+",
  "widgets": [],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "note_taking": {
    "new_note_url": "/booking"
  },
  "scope_extensions": [
    {
      "origin": "https://app-preview-ua.vercel.app"
    }
  ]
